    {% if block('header') is defined %}
     
    {#*** MY SIDE BAR ***#}
    <header class="header">
      <nav class="navigation">
        <ul class="list">
          <li class="brand-logo">
            <a href="">
                <img class="logo-nav" src="{{ asset('build/images/logo.png') }}" alt="profile-picture">
              <span class="title">
              <strong>STUDI FITNESS</strong>
              </span>
            </a>
          </li>
          <li class="">
            {% if is_granted('ROLE_ADMIN') %}
            <a href="{{ path('home') }}">
              <span class="icon">
                <i class="fas fa-tachometer-alt"></i>
              </span>
              <span class="title">Dashboard</span>
            </a>
            {% else %}
            <a href="{{ path('home') }}">
              <span class="icon">
                <i class="fa-solid fa-house"></i>
              </span>
              <span class="title">Mon profil</span>
            </a>
          </li>
          {% endif %}
          {% if is_granted('ROLE_ADMIN') %}
          <li>
            <a href="{{ path('app_user_index') }}">
              <span class="icon">
                <i class="fa fa-users"></i>
              </span>
              <span class="title">Utilisateurs</span>
            </a>
          </li>
          <li>
            <a href="{{ path('app_partner_index') }}">
              <span class="icon">
                <i class="fa fa-handshake"></i>
              </span>
              <span class="title">Partenaires</span>
            </a>
          </li>
          <li>
            <a href="{{ path('app_structure_index') }}">
              <span class="icon">
                <i class="fa fa-building"></i>
              </span>
              <span class="title">Structures</span>
            </a>
          </li>
          {% endif %}
          <li>
            <a href="{{ path('app_logout') }}">
              <span class="icon">
                <i class="fa-solid fa-power-off"></i>
              </span>
              <span class="title">Déconnexion</span>
            </a>
          </li>
        </ul>
      </nav>
    </header>

    {% endif %}

    <main>

    {# Top Bar #}
    {% if block('topbar') is defined %}
    
      <div class="topbar">

        <div class="toggle"></div>

        {# {% if block ('searchBar') is defined %}
        <div class="search">
          <label>
            <input type="text" placeholder="Search...">
            <i class="fa fa-search"></i>
          </label>
        </div>
        {% endif %} #}

        {# Login-Logout #}
        <div class="login-logout">
          <div class="profile">
            <img src="{{ asset('build/images/Jim-Morrison.jpeg') }}" alt="profile-picture">
          </div>
          <div class="menu">
            <ul>
              <li>
              {% if app.user %}
              <i class="fa-solid fa-circle-user"></i>
              {{ app.user.email }}
              {% endif %}
              </li>
              <li class="disconnect">
                <i class="fa-solid fa-power-off"></i>
                <a href="{{ path('app_logout') }}">Déconnexion</a>
              </li>
            </ul>
          </div>
        </div>

      </div>
      {% endif %}

      {% block content %}{% endblock %}
      
    </main>
  
